{
  "name": "myNCI",
  "version": "1.0.2",
  "description": "myNCI Akumina implementation.",
  "main": "index.js",
  "scripts": {
    "start": "SET NODE_OPTIONS=--openssl-legacy-provider && react-scripts start",
    "lang": "webpack --config webpack.lang.config.js",
    "sandbox": "npm run build & npm run setupdev & webpack-dev-server --mode development --no-info --config webserver.config.js",
    "build": "webpack --config webpack.config.js",
    "build-prod-ci": "set NODE_ENV=prod && webpack --config webpack.config.js",
    "package": "akumina-widget-builder package",
    "clean": "akumina-widget-builder clean packages",
    "stub": "akumina-widget-builder stub",
    "config": "akumina-widget-builder config",
    "deploy": "node ./tools/deploy.js",
    "all": "npm run clean & npm run build & npm run package & npm run deploy",
    "vpstub": "akumina-virtualpage-builder stub",
    "initdev": "akumina-widget-builder initdev",
    "setupdev": "akumina-widget-builder setupdev",
    "cdnpackage": "akumina-widget-builder cdnpackage",
    "lint": "npm run lint:src",
    "lint:src": "eslint src --ext .js,.jsx",
    "lint:fix": "npm run lint:src:fix",
    "lint:src:fix": "eslint src --ext .js,.jsx --fix"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/css": "^0.4.0",
    "@eslint/js": "^9.22.0",
    "@types/jquery": "^3.5.32",
    "@types/node": "^22.13.9",
    "clean-css": "^5.3.3",
    "eslint": "^9.22.0",
    "eslint-plugin-react": "^7.37.4",
    "filemanager-webpack-plugin": "^8.0.0",
    "fs": "0.0.1-security",
    "globals": "^16.0.0",
    "jiti": "^2.4.2",
    "path": "^0.12.7",
    "terser-webpack-plugin": "^5.3.14",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.2",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@types/akumina-core": "^5.0.*",
    "akumina-virtualpage-builder": "^1.0.3",
    "akumina-widget-builder": "^2.0.11",
    "dotenv": "^16.4.7",
    "exe": "^1.0.2"
  }
}
